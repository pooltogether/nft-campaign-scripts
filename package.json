{
  "name": "graphql-query-template",
  "version": "1.0.0",
  "description": "Template repository to kickstart a project to query GraphQL endpoints",
  "main": "index.js",
  "repository": "git@github.com:pooltogether/graphql-query-template.git",
  "author": "PoolTogether Inc.",
  "license": "GPL-3.0",
  "scripts": {
    "format": "prettier --config .prettierrc --write \"**/*.{ts,js}\"",
    "format:file": "prettier --config .prettierrc --write",
    "compute:season-one:week-one": "ts-node ./season/one/scripts/week-one.ts",
    "compute:season-one:week-four": "ts-node ./season/one/scripts/week-four.ts"
  },
  "dependencies": {
    "graphql": "16.3.0",
    "graphql-request": "4.1.0",
    "json2csv": "5.0.7"
  },
  "devDependencies": {
    "@types/json2csv": "5.0.3",
    "prettier": "2.6.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.2"
  }
}
